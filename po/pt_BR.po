#: gnome-initial-setup/pages/live-chooser/gis-live-chooser-page.c:175
#: gnome-initial-setup/pages/live-chooser/gis-live-chooser-page.ui:42
msgid "Try Endless OS by running it from the USB Stick."
msgstr "Teste o Endless OS rodando o sistema em um pendrive"

#: gnome-initial-setup/pages/live-chooser/gis-live-chooser-page.c:176
#: gnome-initial-setup/pages/live-chooser/gis-live-chooser-page.ui:61
msgid "Try Endless OS by running it from the DVD."
msgstr "Teste o Endless OS à partir de um DVD."

#: gnome-initial-setup/pages/live-chooser/gis-live-chooser-page.c:177
#: gnome-initial-setup/pages/live-chooser/gis-live-chooser-page.ui:147
msgid "Reformat this computer with Endless OS."
msgstr "Formatar este computador com o Endless OS"

#: gnome-initial-setup/pages/live-chooser/gis-live-chooser-page.c:178
#: gnome-initial-setup/pages/live-chooser/gis-live-chooser-page.ui:102
msgid "Try It"
msgstr "Experimente"

#: gnome-initial-setup/pages/live-chooser/gis-live-chooser-page.c:179
#: gnome-initial-setup/pages/live-chooser/gis-live-chooser-page.ui:176
msgid "Reformat"
msgstr "Formatar"

#: gnome-initial-setup/pages/live-chooser/gis-live-chooser-page.c:181
msgid "Try or Reformat"
msgstr "Teste ou Reformate"

